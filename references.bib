	issn = {0004-6361},
	url = {https://ui.adsabs.harvard.edu/abs/2011A%26A...530A.151C/abstract},
	doi = {10.1051/0004-6361/201116567},
	abstract = { Aims: What is the origin of the large-amplitude variability in Wolf-Rayet WN8 stars in general and WR123 in particular? A dedicated spectroscopic campaign targets the ten-hour period previously found in the high-precision photometric data obtained by the MOST satellite.  Methods: In June-August 2003 we obtained a series of high signal-to-noise, mid-resolution spectra from several sites in the λλ 4000-6940 Å domain. We also followed the star with occasional broadband (Johnson V) photometry. The acquired spectroscopy allowed a detailed study of spectral variability on timescales from {\textasciitilde}5 min to months.  Results: We find that all observed spectral lines of a given chemical element tend to show similar variations and that there is a good correlation between the lines of different elements, without any significant time delays, save the strong absorption components of the Hei lines, which tend to vary differently from the emission parts. We find a single sustained periodicity, P {\textasciitilde} 9.8 h, which is likely related to the relatively stable pulsations found in MOST photometry obtained one year later. In addition, seemingly stochastic, large-amplitude variations are also seen in all spectral lines on timescales of several hours to several days. {\textless}P /{\textgreater}Based on observations obtained at the Canada-France-Hawaii Telescope (CFHT) which is operated by the National Research Concil of Canada, the Institut National des Sciences de l'Univers of the Centre National de Recherche Scientifique of France, and the University of Hawaii. Based also on observations made with ESO Telescopes at the La Silla Observatory, under programme ID 271.D-5025.Photometric data presented in Fig. 1 are only available at the CDS via anonymous ftp to cdsarc.u-strasbg.fr (130.79.128.5) or via {\textless}A href="http://cdsarc.u-strasbg.fr/viz-bin/qcat?J/A+A/530/A151"{\textgreater}http://cdsarc.u-strasbg.fr/viz-bin/qcat?J/A+A/530/A151{\textless}/A{\textgreater}},
	language = {en},
	urldate = {2022-05-20},
	journal = {Astronomy \&amp; Astrophysics, Volume 530, id.A151,   6   pp.},
	author = {Chené, A.-N. and Foellmi, C. and Marchenko, S. V. and St-Louis, N. and Moffat, A. F. J. and Ballereau, D. and Chauville, J. and Zorec, J. and Poteet, C. A.},
	month = jun,
	year = {2011},
	pages = {A151},
	file = {Full Text PDF:/Users/karansinghd/Zotero/storage/26B9WQQK/Chené et al. - 2011 - A 10-h period revealed in optical spectra of the h.pdf:application/pdf},
}

@article{moffat_fastest_1982,
	title = {The fastest runaway {Wolf}-rayet star of population {I} in the galaxy, 209 {BAC} : evidence for a low mass companion.},
