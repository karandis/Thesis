	month = feb,
	year = {2011},
	pages = {A69},
	file = {Full Text PDF:/Users/karansinghd/Zotero/storage/TNDT9Y6D/Raskin et al. - 2011 - HERMES a high-resolution fibre-fed spectrograph f.pdf:application/pdf;Snapshot:/Users/karansinghd/Zotero/storage/FENLVYS9/abstract.html:text/html},
