	keywords = {Astronomy, Interstellar Extinction, Stellar Magnitude, Stellar Spectrophotometry, Ubv Spectra, Spectral Energy Distribution, Radiant Flux Density, Wolf-Rayet Stars, Astronomical Catalogs, Northern Sky, Stellar Color},
