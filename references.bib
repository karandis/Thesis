	note = {ADS Bibcode: 2021MNRAS.507.5348V},
	keywords = {Astrophysics - Solar and Stellar Astrophysics, binaries: spectroscopic, Magellanic Clouds, open clusters and associations: individual: 30 Doradus, stars: early-type, stars: massive},
	pages = {5348--5375},
	file = {Full Text:/Users/karansinghd/Zotero/storage/LWIN3P4C/Villase{\~n}or et al. - 2021 - The B-type binaries characterization programme I. .pdf:application/pdf},
