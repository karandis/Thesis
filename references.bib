	keywords = {Astrophysics, Galaxy:HR Diagrams, Galaxy:Supergiants, Hertzsprung-Russell Diagram, HR Diagrams:Magellanic Clouds, HR Diagrams:Supergiants, Magellanic Clouds, Magellanic Clouds:Supergiants, Stellar Evolution, Stellar Luminosity, Stellar Mass Ejection, Stellar Spectra, Supergiant Stars, Supermassive Stars},
