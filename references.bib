	pages = {444},
	file = {Accepted Version:/Users/karansinghd/Zotero/storage/7FC394HM/Sana et al. - 2012 - Binary Interaction Dominates the Evolution of Mass.pdf:application/pdf},
}

@article{paczynski_evolution_1967,
