	note = {ADS Bibcode: 2012AJ....143..149S},
	keywords = {Astrophysics - Astrophysics of Galaxies, Be, Galaxy: disk, Galaxy: stellar content, infrared: stars, stars: emission-line, stars: Wolf-Rayet, surveys},
	pages = {149},
	file = {Submitted Version:/Users/karansinghd/Zotero/storage/G7TIT4RX/Shara et al. - 2012 - A Near-infrared Survey of the Inner Galactic Plane.pdf:application/pdf},
}

@article{shara_near-infrared_2009,
	title = {A {Near}-{Infrared} {Survey} of the {Inner} {Galactic} {Plane} for {Wolf}-{Rayet} {Stars}. {I}. {Methods} and {First} {Results}: 41 {New} {WR} {Stars}},
	volume = {138},
	issn = {0004-6256},
	shorttitle = {A {Near}-{Infrared} {Survey} of the {Inner} {Galactic} {Plane} for {Wolf}-{Rayet} {Stars}. {I}. {Methods} and {First} {Results}},
	url = {https://ui.adsabs.harvard.edu/abs/2009AJ....138..402S},
	doi = {10.1088/0004-6256/138/2/402},
	abstract = {The discovery of new Wolf-Rayet (WR) stars in our Galaxy via large-scale narrowband optical surveys has been severely limited by dust extinction. Recent improvements in infrared technology have made narrowband-broadband imaging surveys viable again. We report a new J, K, and narrowband imaging survey of 300 deg2 of the plane of the Galaxy, spanning 150 degrees in Galactic longitude and reaching 1 degree above and below the Galactic plane. The survey has a useful limiting magnitude of K = 15 over most of the observed Galactic plane, and K = 14 within a few degrees of the Galactic center. Thousands of emission line candidates have been detected. In spectrographic follow-ups of 173 WR star candidates we have discovered 41 new WR stars, 15 of type WN and 26 of type WC. Star subtype assignments have been confirmed with K-band spectra, and distances approximated using the method of spectroscopic parallax. A few of the new WR stars are among the most distant known in our Galaxy. The distribution of these new WR stars is seen to follow that of previously known WR stars along the spiral arms of the Galaxy. Tentative radial velocities were also measured for most of the new WR stars.},
	urldate = {2022-04-04},
	journal = {The Astronomical Journal},
	author = {Shara, Michael M. and Moffat, Anthony F. J. and Gerke, Jill and Zurek, David and Stanonik, Kathryn and Doyon, Ren{\'e} and Artigau, Etienne and Drissen, Laurent and Villar-Sbaffi, Alfredo},
	month = aug,
	year = {2009},
	note = {ADS Bibcode: 2009AJ....138..402S},
	keywords = {Astrophysics - Astrophysics of Galaxies, Be, Galaxy: disk, Galaxy: stellar content, stars: emission-line, stars: Wolf-Rayet, surveys},
	pages = {402--420},
	file = {Submitted Version:/Users/karansinghd/Zotero/storage/KYG2Q7PZ/Shara et al. - 2009 - A Near-Infrared Survey of the Inner Galactic Plane.pdf:application/pdf},
}

