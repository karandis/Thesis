	keywords = {STARS: BINARIES: SPECTROSCOPIC, STARS: INDIVIDUAL HENRY DRAPER NUMBER: HD 197406, STARS: WOLF-RAYET},
	pages = {386},
	file = {Full Text PDF:/Users/karansinghd/Zotero/storage/DM8UPFSX/Marchenko et al. - 1996 - The Wolf-Rayet Star HD 197406, with Its Strongly I.pdf:application/pdf},
}

