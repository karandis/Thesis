@article{vink_mass-loss_2001,
	title = {Mass-loss predictions for {O} and {B} stars as a function of metallicity},
	volume = {369},
	issn = {0004-6361},
	url = {https://ui.adsabs.harvard.edu/abs/2001A%26A...369..574V/abstract},
	doi = {10.1051/0004-6361:20010127},
	abstract = {We have calculated a grid of massive star wind models and mass-loss rates for a wide range of metal abundances between 1/100 \&lt;= Z/Z{\textless}SUB{\textgreater}sun{\textless}/SUB{\textgreater} \&lt;= 10. The calculation of this grid completes the Vink et al. (2000) mass-loss recipe with an additional parameter Z. We have found that the exponent of the power law dependence of mass loss vs. metallicity is constant in the range between 1/30 \&lt;= Z/Z{\textless}SUB{\textgreater}sun{\textless}/SUB{\textgreater} \&lt;= 3. The mass-loss rate scales as dot M {\textasciitilde} Z{\textless}SUP{\textgreater}0.85{\textless}/SUP{\textgreater} v{\textless}SUB{\textgreater}infty {\textless}/SUB{\textgreater}{\textless}SUP{\textgreater}p{\textless}/SUP{\textgreater} with p = -1.23 for stars with T\_eff \&gt;{\textasciitilde} 25 000 K, and p = -1.60 for the B supergiants with T\_eff \&lt;{\textasciitilde} 25 000 K. Taking also into account the metallicity dependence of v{\textless}SUB{\textgreater}infty {\textless}/SUB{\textgreater}, using the power law dependence v{\textless}SUB{\textgreater}infty {\textless}/SUB{\textgreater} {\textasciitilde} Z{\textless}SUP{\textgreater}0.13{\textless}/SUP{\textgreater} from Leitherer et al. (1992), the overall result of mass loss as a function of metallicity can be represented by dot M {\textasciitilde} Z{\textless}SUP{\textgreater}0.69{\textless}/SUP{\textgreater} for stars with T\_eff \&gt;{\textasciitilde} 25 000 K, and dot M {\textasciitilde} Z{\textless}SUP{\textgreater}0.64{\textless}/SUP{\textgreater} for B supergiants with T\_eff \&lt;{\textasciitilde} 25 000 K. Although it is derived that the exponent of the mass loss vs. metallicity dependence is constant over a large range in Z, one should be aware of the presence of bi-stability jumps at specific temperatures. Here the character of the line driving changes drastically due to recombinations of dominant metal species resulting in jumps in the mass loss. We have investigated the physical origins of these jumps and have derived formulae that combine mass loss recipes for both sides of such jumps. As observations of different galaxies show that the ratio Fe/O varies with metallicity, we make a distinction between the metal abundance Z derived on the basis of iron or oxygen lines. Our mass-loss predictions are successful in explaining the observed mass-loss rates for Galactic and Small Magellanic Cloud O-type stars, as well as in predicting the observed Galactic bi-stability jump. Hence, we believe that our predictions are reliable and suggest that our mass-loss recipe be used in future evolutionary calculations of massive stars at different metal abundance. A computer routine to calculate mass loss is publicly available.},
	language = {en},
	urldate = {2022-05-23},
	journal = {Astronomy and Astrophysics, v.369, p.574-588 (2001)},
