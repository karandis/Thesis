@article{hamann_temperature_2003,
	title = {A temperature correction method for expanding atmospheres},
	volume = {410},
	issn = {0004-6361},
	url = {https://ui.adsabs.harvard.edu/abs/2003A%26A...410..993H/abstract},
	doi = {10.1051/0004-6361:20031308},
	abstract = {Model atmospheres form the basis for the interpretation of stellar spectra. A major problem in those model calculations is to establish the temperature stratification from the condition of radiative equilibrium. Dealing with non-LTE models for spherically expanding atmospheres of Wolf-Rayet stars, we developed a new temperature correction method. Its basic idea dates back to 1955 when it was proposed by Unsöld for grey, static and plane-parallel atmospheres in LTE. The equations were later generalized to the non-grey case by Lucy. In the present paper we furthermore drop the Eddington approximation, proceed to spherical geometry and allow for expansion of the atmosphere. Finally the concept of an ``approximate lambda operator'' is employed to speed up the convergence. Tests for Wolf-Rayet type models demonstrate that the method works fine even in situations of strong non-LTE.},
	language = {en},
	urldate = {2022-04-09},
	journal = {Astronomy and Astrophysics, v.410, p.993-1000 (2003)},
	author = {Hamann, W.-R. and Gräfener, G.},
	month = nov,
	year = {2003},
	pages = {993},
	file = {Full Text PDF:/Users/karansinghd/Zotero/storage/9EEJKP7J/Hamann and Gräfener - 2003 - A temperature correction method for expanding atmo.pdf:application/pdf},
}

@article{hamann_grids_2004,
	title = {Grids of model spectra for {WN} stars, ready for use},
	volume = {427},
