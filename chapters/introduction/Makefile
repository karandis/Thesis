MAINDIR = /home/rajeev/Thesis

.PHONY: introduction.pdf
introduction.pdf: 
	( cd "$(MAINDIR)" && make chapters/introduction/$@ )

.PHONY: bare introduction_bare.pdf
bare: introduction_bare.pdf
introduction_bare.pdf: 
	( cd "$(MAINDIR)" && make chapters/introduction/$@ )

.PHONY: image
image: 
	@if [ -f image/Makefile ]; then (cd image  && make); \
		else echo No Makefile found for /home/rajeev/Thesis/image ...; fi;

.PHONY: figurelist
figurelist:
	@( cd $(MAINDIR) && make --no-print-directory $@ TEXFILE=chapters/introduction/introduction.tex )

.PHONY: clean
clean:
	$(RM) $(foreach ext, .toc .aux .log .bbl .blg .log .lof .lot .ilg .out .glo .gls .nlo .nls .brf .ist .glg .synctex.gz .tgz .idx .ind -blx.bib .fdb_latexmk .run.xml .bcf, $(wildcard introduction*$(ext)))

.PHONY: realclean
realclean: clean
	$(RM) introduction.pdf introduction_bare.pdf

s:
	@echo gvim introduction.tex allpapers.bib > $@
	@chmod +x $@
	@echo You can now start using ./s
