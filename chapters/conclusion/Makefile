		else echo No Makefile found for /home/rajeev/Thesis/image ...; fi;

.PHONY: figurelist
figurelist:
	@( cd $(MAINDIR) && make --no-print-directory $@ TEXFILE=chapters/conclusion/conclusion.tex )

.PHONY: clean
clean:
	$(RM) $(foreach ext, .toc .aux .log .bbl .blg .log .lof .lot .ilg .out .glo .gls .nlo .nls .brf .ist .glg .synctex.gz .tgz .idx .ind -blx.bib .fdb_latexmk .run.xml .bcf, $(wildcard conclusion*$(ext)))

.PHONY: realclean
realclean: clean
	$(RM) conclusion.pdf conclusion_bare.pdf

s:
	@echo gvim conclusion.tex allpapers.bib > $@
	@chmod +x $@
	@echo You can now start using ./s
