.PHONY: conclusion.pdf
conclusion.pdf: 
	( cd "$(MAINDIR)" && make chapters/conclusion/$@ )

.PHONY: bare conclusion_bare.pdf
bare: conclusion_bare.pdf
conclusion_bare.pdf: 
	( cd "$(MAINDIR)" && make chapters/conclusion/$@ )

.PHONY: image
image: 
