MAINDIR = /home/rajeev/Thesis

.PHONY: manual.pdf
manual.pdf: 
	( cd "$(MAINDIR)" && make chapters/manual/$@ )

.PHONY: bare manual_bare.pdf
bare: manual_bare.pdf
manual_bare.pdf: 
	( cd "$(MAINDIR)" && make chapters/manual/$@ )

.PHONY: image
image: 
	@if [ -f image/Makefile ]; then (cd image  && make); \
		else echo No Makefile found for /home/rajeev/Thesis/image ...; fi;

.PHONY: figurelist
figurelist:
	@( cd $(MAINDIR) && make --no-print-directory $@ TEXFILE=chapters/manual/manual.tex )

.PHONY: clean
clean:
	$(RM) $(foreach ext, .toc .aux .log .bbl .blg .log .lof .lot .ilg .out .glo .gls .nlo .nls .brf .ist .glg .synctex.gz .tgz .idx .ind -blx.bib .fdb_latexmk .run.xml .bcf, $(wildcard manual*$(ext)))

.PHONY: realclean
realclean: clean
	$(RM) manual.pdf manual_bare.pdf

s:
	@echo gvim manual.tex allpapers.bib > $@
	@chmod +x $@
	@echo You can now start using ./s
